package com.bridgelabz.utility;


public class LinkedList 
{
	Node start;
	Node end;
	int size;
	
	public LinkedList emptyList()
	{
		this.start=null;
		this.end=null;
		this.size=0;
		return this; 
	}
	
	public void add(int value)
	{
		Node newnode=new Node(value, null);
		size++;
		if(start==null)
		{
			start=newnode;
			end=start;
		}
		else
		{
			end.setPointer(start);
			end=newnode;
		}
	}
	public void remove(int value)
	{
		
	}
}