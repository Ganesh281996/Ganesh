package com.bridgelabz.utility;

class Node<T>
{
	private int value;
	private Node pointer;
	
	public Node(int value, Node pointer) 
	{
		this.value = value;
		this.pointer = pointer;
	}

	public int getValue() 
	{
		return value;
	}

	public void setValue(int value) 
	{
		this.value = value;
	}

	public Node getPointer() 
	{
		return pointer;
	}

	public void setPointer(Node pointer) 
	{
		this.pointer = pointer;
	}	
}