package com.bridgelabz.utility;

public interface SocketAdapter 
{
	Volt get120Volt();
	Volt get3Volt();
}

class SocketClassAdapter extends Socket implements SocketAdapter
{

	@Override
	public Volt get120Volt() 
	{
		return new Volt(3);
	}

	@Override
	public Volt get3Volt() 
	{
		// TODO Auto-generated method stub
		return null;
	}
	
}