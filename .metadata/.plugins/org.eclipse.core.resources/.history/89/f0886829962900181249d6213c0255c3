package com.bridgelabz.objectoriented;

import org.json.simple.JSONObject;

import com.bridgelabz.utility.Clinique;

public class CliniqueManagement 
{
	public static void main(String[] args) 
	{
		Clinique c=new Clinique();
		
//		c.addDoctor("a", 2, "ff", "dfsdf", 0);
//		c.addPatient("a", 3, 31, 43645635);
		
		JSONObject jsonObject=c.searchPatientById(1);
		if(jsonObject==null)
		{
			System.out.println("Patient Id was not found");
		}
		else
		{
			System.out.println(jsonObject);
		}
	}
}